        "import numpy as np\n",
        "\n",
        "# Create indices for i and j\n",
        "i = np.arange(5).reshape(-1, 1)  # Column vector\n",
        "j = np.arange(5).reshape(1, -1)  # Row vector\n",
        "\n",
        "# Compute A[i, j] = i^2 + j^2\n",
        "A = i**2 + j**2\n",
        "\n",
        "print(\"Generated Array:\")\n",
        "print(A)\n"
